<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>BulkMail | List Uploads</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
  </head>
  <body>
    <%include nav.ejs%>
    <div class="row">
      <div class="col-xs-12">
        <h1 align="center">Your List</h1>
        <hr />
        <br />
      </div>
    </div>
    <div>
      <table class="fl-table">
        <thead>
          <tr>
            <th>Email</th>
            <th>Role</th>
            <th>Status</th>
          </tr>
        </thead>
        <tbody>
          <% users.forEach(function(email){ %>
          <tr>
            <td data-column="Email">
              <%= email.email %>
            </td>
            <td data-column="Role">
              <%= email.role %>
            </td>
            <td data-column="Status">
              <%= email.flag %>
            </td>
          </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
  </body>
</html>
